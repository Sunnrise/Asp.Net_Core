 *@
 <h3>ViewBag</h3>
<ul> @*We assign the type of the ViewBag.products because it will be assign in the runtime but it will be slower than that*@
    @foreach (var product in ViewBag.products as List<View_and_Razor.Models.Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>
<h3>ViewData</h3>
<ul>
    @foreach (var product in ViewData["products"] as List<View_and_Razor.Models.Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>

<h3>TempData</h3>
<ul>
    @foreach (var product in TempData["products"] as List<View_and_Razor.Models.Product>)
    {
        <li>@product.ProductName</li>
    }
</ul>